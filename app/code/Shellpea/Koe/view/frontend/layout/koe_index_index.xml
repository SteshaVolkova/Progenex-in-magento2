<?xml version="1.0"?>
<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head> 
		<title>Some page</title> 
	</head>
    <body>
        <referenceContainer name="content">
            <block class="Shellpea\Koe\Block\Index" name="koe_index_index" template="Shellpea_Koe::index.phtml">
                <arguments>
                    <argument name="width" xsi:type="string">700px</argument>
                    <argument name="mina" xsi:type="string">8</argument>
                    <argument name="maxa" xsi:type="string">30</argument>
                </arguments>
            </block>
        </referenceContainer>
        <referenceBlock name="koe_display" remove="true" />
    </body>
</page>
